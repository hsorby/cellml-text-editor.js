{
  "name": "cellml-text-editor",
  "version": "0.2.0",
  "description": "A CellML text editor library for parsing and generating CellML text representations.",
  "keywords": [
    "cellml",
    "text editor",
    "parser",
    "generator",
    "latex"
  ],
  "author": {
    "name": "Hugh Sorby",
    "email": "h.sorby@auckland.ac.nz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hsorby/cellml-text-editor.js.git"
  },
  "license": "Apache-2.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/cellml-text-editor.umd.cjs",
  "module": "./dist/cellml-text-editor.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/cellml-text-editor.js",
      "require": "./dist/cellml-text-editor.umd.cjs"
    }
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type:check \"build:only {@}\" --",
    "preview": "vite preview",
    "build:only": "vite build",
    "type:check": "vue-tsc --build",
    "build:grammar": "lezer-generator src/grammar/cellml.lezer -o src/grammar/parser.ts && echo '// @ts-nocheck' | cat - src/grammar/parser.ts > temp && mv temp src/grammar/parser.ts"
  },
  "devDependencies": {
    "@babel/runtime": "^7.28.6",
    "@codemirror/language": "^6.12.1",
    "@codemirror/state": "^6.5.4",
    "@codemirror/view": "^6.39.13",
    "@lezer/generator": "^1.8.0",
    "@lezer/highlight": "^1.2.3",
    "@lezer/lr": "^1.4.8",
    "@tsconfig/node24": "^24.0.3",
    "@types/katex": "^0.16.8",
    "@types/node": "^24.10.4",
    "@uiw/codemirror-theme-sublime": "^4.25.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/tsconfig": "^0.8.1",
    "codemirror": "^6.0.2",
    "katex": "^0.16.27",
    "npm-run-all2": "^8.0.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "~5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue": "^3.5.26",
    "vue-codemirror": "^6.1.1",
    "vue-tsc": "^3.2.2",
    "vue3-libcellml.js": "^0.6.3"
  }
}
